{
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run -A --watch server.ts",
    "start": "deno run -A server.ts",
    "check": "deno check js/*.ts",
    "check-all": "deno check --remote js/*.ts server.ts",
    "deps": "./scripts/get_deps.sh",
    "deps-force": "./scripts/get_deps.sh --force"
  },
  "compilerOptions": {
    "lib": [
      "ES2022",
      "DOM"
    ],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true
  },
  "deploy": {
    "project": "0769dee0-07b3-41f4-be92-3f89447a0c4b",
    "exclude": [
      "**/node_modules"
    ],
    "include": [],
    "entrypoint": "server.ts"
  }
}